{
  "formats": {
    "input": "svg",
    "output": "jpg"
  },
  "paths": {
    "input": "input/sheets/",
    "shapes": "output/shapes/",
    "generated": "output/generated/",
    "inkscape": "/Applications/Inkscape.app/Contents/MacOS/inkscape"
  },
  "output": {
    "count": 2,
    "width": 250,
    "height": 250,
    "padding": 10,
    "shape_scale_min": 1,
    "shape_scale_max": 2
  },
  "options": {
    "separateAnnotationFolder": false,
    "includeValidationSet": false
  }
}
