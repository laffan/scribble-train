{
  "formats": {
    "input": "svg",
    "output": "jpg"
  },
  "paths": {
    "input": "input/sheets/",
    "shapes": "output/shapes/",
    "generated": "output/generated/",
    "inkscape": "/Applications/Inkscape.app/Contents/MacOS/inkscape"
  },
  "output": {
    "count": 70,
    "width": 400,
    "height": 300,
    "shapeCount": [1, 3],
    "scale": [0.5, 0.8],
    "padding": 5
  },
  "options": {
    "separateAnnotationFolder": true,
    "includeValidationSet": true
  }
}
